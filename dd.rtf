  <script>\
    const stage = document.getElementById('stage');\
    const heartsWrap = document.getElementById('hearts');\
    const qEl = document.getElementById('question');\
    const answersEl = document.getElementById('answers');\
    const bar = document.getElementById('bar');\
    const modal = document.getElementById('modal');\
\
    const questions = [\
      "Do you like spending time together?",\
      "Do you laugh at my silly jokes?",\
      "Do I make you feel special?",\
      "Would you like a surprise date?",\
      "Will you be my Valentine?"\
    ];\
\
    let idx = 0;\
    function render()\{\
      qEl.textContent = questions[idx];\
      answersEl.innerHTML = '';\
      const yes = document.createElement('button');\
      yes.className = 'btn yes';\
      yes.textContent = 'Yes \uc0\u55357 \u56469 ';\
      yes.onclick = ()=>\{ if(idx===questions.length-1)\{showYes()\} else \{ idx++; updateProgress(); render(); playTinyPop(); \} \};\
\
      const no = document.createElement('button');\
      no.className = 'btn no';\
      no.textContent = 'No';\
\
      // special behavior on 5th question (index 4)\
      if(idx === questions.length-1)\{\
        no.addEventListener('mouseenter', runaway);\
        no.addEventListener('focus', runaway);\
        no.addEventListener('click', (e)=>\{ e.preventDefault(); runaway(); \});\
      \} else \{\
        no.onclick = ()=>\{ idx = Math.max(0, idx-1); updateProgress(); render(); \}\
      \}\
\
      answersEl.appendChild(yes);\
      answersEl.appendChild(no);\
    \}\
\
    function updateProgress()\{\
      const pct = Math.round(((idx)/(questions.length-1))*100);\
      bar.style.width = pct + '%';\
    \}\
\
    function showYes()\{\
      modal.style.display = 'flex';\
      spawnConfetti(90);\
      playCelebrate();\
    \}\
\
    function runaway(e)\{\
      const card = document.getElementById('card');\
      const btn = e?.currentTarget || event?.target;\
      const rect = card.getBoundingClientRect();\
      const bw = btn.offsetWidth, bh = btn.offsetHeight;\
      const x = Math.random()*(rect.width - bw - 40) + 20;\
      const y = Math.random()*(rect.height - bh - 80) + 20;\
      btn.style.position = 'absolute';\
      btn.style.left = x + 'px';\
      btn.style.top = y + 'px';\
      btn.style.transition = 'left .25s ease, top .25s ease';\
      playTinySwoosh();\
      burstHeartsAt(x + bw/2, y + bh/2);\
    \}\
\
    function seedHearts(n=24)\{\
