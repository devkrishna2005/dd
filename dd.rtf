      for(let i=0;i<6;i++)\{\
        const h = document.createElement('div');\
        h.className = 'heart';\
        const size = Math.random()*26 + 12;\
        h.style.width = size+'px'; h.style.height = size+'px';\
        h.style.left = (cx + (Math.random()-0.5)*120) + 'px';\
        h.style.top = (cy + (Math.random()-0.5)*120) + 'px';\
        h.style.opacity = 1;\
        h.innerHTML = `<svg viewBox="0 0 32 29" width="$\{size\}" height="$\{size\}"><path d="M23.6 0c-3.1 0-5.6 2-7.6 4.2C14 2 11.6 0 8.6 0 3.7 0 0 4 0 9.2c0 9.9 16 19.8 16 19.8S32 19.1 32 9.2C32 4 28.3 0 23.6 0z" fill="#ff6fb5"></path></svg>`;\
        heartsWrap.appendChild(h);\
